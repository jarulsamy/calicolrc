#: ../Source/Calico/Main.cs:319 ../Source/Calico/Main.cs:312
msgid "Start calico with the following options:"
msgstr "Inicia Calico con las siguientes opciones:"

#: ../Source/Calico/Main.cs:320 ../Source/Calico/Main.cs:313
msgid "  StartCalico                               Defaults to shell"
msgstr ""
"  StartCalico                               Inicia en el Ambiente de Comandos"

#: ../Source/Calico/Main.cs:321 ../Source/Calico/Main.cs:314
msgid ""
"  StartCalico FILENAME:LINE ...             Edits FILENAMEs, positioned on "
"LINEs"
msgstr ""
"  StartCalico ARCHIVOs:LINEA ...            Edita el/los ARCHIVOs, "
"ubicándose en la LINEA"

#: ../Source/Calico/Main.cs:322 ../Source/Calico/Main.cs:315
msgid ""
"  StartCalico --lang=LANGUAGE               Sets default language (python, "
"etc.)"
msgstr ""
"  StartCalico --lang=LENGUAJE               Establece el lenguaje por "
"defecto (python, etc)"

#: ../Source/Calico/Main.cs:323 ../Source/Calico/Main.cs:316
msgid "  StartCalico --chat                        Brings up chat window"
msgstr "  StartCalico --chat                        Muestra la ventana de Chat"

#: ../Source/Calico/Main.cs:324 ../Source/Calico/Main.cs:317
msgid ""
"  StartCalico --exec FILENAMEs              Run FILENAMEs standalone with "
"graphics"
msgstr ""
"  StartCalico --exec ARCHIVOs               Ejecuta sólo el/los ARCHIVOs, "
"con gráficos"

#: ../Source/Calico/Main.cs:325 ../Source/Calico/Main.cs:318
msgid ""
"  StartCalico --repl FILENAMEs              Run FILENAMEs standalone with "
"graphics and then start read-eval loop"
msgstr ""
"  StartCalico --repl ARCHIVOs               Ejecuta sólo el/los ARCHIVOs, "
"con gráficos e inicia un ciclo de lectura-evaluación"

#: ../Source/Calico/Main.cs:326 ../Source/Calico/Main.cs:319
msgid ""
"  StartCalico --exec --nographics FILENAMEs Run FILENAMEs standalone without "
"graphics"
msgstr ""
"  StartCalico --exec --nographics ARCHIVOs  Ejecuta sólo el/los ARCHIVOs, "
"sin gráficos"

#: ../Source/Calico/Main.cs:327 ../Source/Calico/Main.cs:320
msgid ""
"  StartCalico --repl --nographics FILENAMEs Run FILENAMEs standalone without "
"graphics and then start read-eval loop"
msgstr ""
"  StartCalico --repl --nographics ARCHIVOs  Ejecuta sólo el/los ARCHIVOS, "
"sin gráficos e inicia un ciclo de lectura-evaluación"

#: ../Source/Calico/Main.cs:328 ../Source/Calico/Main.cs:321
#, csharp-format
msgid ""
"  StartCalico --version                     Displays the version number ({0})"
msgstr ""
"  StartCalico --version                     Despliega el número de la "
"versión ({0})"

#: ../Source/Calico/Main.cs:329 ../Source/Calico/Main.cs:324
msgid ""
"  StartCalico --reset                       Resets config settings to "
"factory defaults"
msgstr ""
"  StartCalico --reset                       Restaura la configuración a sus "
"valores por defecto"

